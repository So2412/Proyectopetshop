<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <script src="js/jquery-3.6.0.min.js"></script>
    <script src="js/actualizarusu.js"></script>
    <script src="js/eliminarusu.js"></script>
    <link href="css/administrador.css" rel="stylesheet">
    <link rel="preconnect" href="https://fonts.gstatic.com">
<link href="https://fonts.googleapis.com/css2?family=Lato&display=swap" rel="stylesheet">
    <title>Usuario</title>
</head>
<body>

    <section class="centrar-imagen">
        <img src="ima/logo.PNG" class="logo-menu">
    </section>
    <header class="Tituloosm">   
        <section class="barra">
            <h2>Menu<img src="ima/lista_icono2.png" alt=""></h2>
            <ul>
                <li><a href="/pedido">Pedido</a></li>
                <li><a href="/proveedor">Proveedor</a></li>
                <li><a href="/servicios">Servicios</a></li>
                <li><a href="/usuarios">Usuarios</a></li>
                <li><a href="/categoria">Categoria</a></li>
                <li><a href="/cita">Cita</a></li>
                <li><a href="/detallecita">Detalle cita</a></li>
                <li><a href="/detallepedido">Detalle pedido</a></li>
                <li><a href="/dueno">Due単o</a></li>
                <li><a href="/insumos">Insumos</a></li>
                <li><a href="/mascota">Mascota</a></li>
                <li><a href="/producto">Producto</a></li>
                <li><a href="/ventas">Ventas</a></li>
                <li><a href="/veterinario">Veterinario</a></li>
            </ul>
           
        </section> 
    </header>
    <nav class="menu">
        <a href="cerrar">Cerrar sesion</a>
    </nav>
    <div class="login-box">
        <h1>Usuarios</h1>
        <form action="/frminsertarusuarios" method="POST"> 
          <!--  INPUT -->
          <input type="text" name="UsuDoc"         class="" placeholder="Documento usuario">
          <input type="text" name="UsuTipoDoc"     class="" placeholder="Tipo de documento">
          <input type="text" name="UsuNombre"      class="" placeholder="Nombre">
          <input type="text" name="UsuApellido"    class="" placeholder="Apellido">
          <input type="text" name="UsuUsuario"     class="" placeholder="Usuario">
          <input type="text" name="UsuContrase単a"  class="" placeholder="Contrase単a">
          <input type="text" name="UsuRol"         class="" placeholder="Rol">
          <!--  BUTTON -->
          <button type="submit">Insertar</button>
        </form>
    </div>

    <from action="" name="frm" id="frm" class="tabla">
        <section class="scroll">
        <table class="table">
            <thead>
                <tr>
                    <th>DOCUMENTO</th>
                    <th>TIPO DOC</th>
                    <th>NOMBRE</th>
                    <th>APELLIDO</th>
                    <th>USUARIO</th>
                    <th>PASSWORD</th>
                    <th>ROL</th>
                    <th>ACTUALIZAR</th>
                    <th>ELIMINAR</th>
                </tr>
            </thead>
    
            <tbody>
             <% datos.forEach(function (datos){ %>    
                <tr>
                    <td><input type="text" value="<%=datos.UsuDoc%>" name="doc" id="doc" class="doc"></td>
                    <td><input type="text" value="<%=datos.UsuTipoDoc%>" name="ti" id="ti" class="ti"></td>
                    <td><input type="text" value="<%=datos.UsuNombre%>" name="no" id="no" class="no"></td>                
                    <td><input type="text" value="<%=datos.UsuApellido%>" name="ap" id="ap" class="ap"></td> 
                    <td><input type="text" value="<%=datos.UsuUsuario%>" name="us" id="us" class="us"></td> 
                    <td><input type="text" value="<%=datos.UsuContrase単a%>" name="co" id="co" class="co"></td> 
                    <td><input type="text" value="<%=datos.UsuRol%>" name="ro" id="ro" class="ro"></td> 
                    <td><input type="submit" value="Actualizar" name="btnact" id="btnact" class="btnact"></td>
                    <td><input type="submit" value="Eliminar" name="btneli" id="btneli" class="btneli"></td>                                
                </tr>
             <%})%>
            </tbody>
        </table>
    </section>
    </from>

</body>
</html>