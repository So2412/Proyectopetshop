<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <script src="js/jquery-3.6.0.min.js"></script>
    <script src="js/actualizarinsu.js"></script>
    <link href="css/administrador.css" rel="stylesheet">
    <link rel="preconnect" href="https://fonts.gstatic.com">
<link href="https://fonts.googleapis.com/css2?family=Lato&display=swap" rel="stylesheet">
    <title>Usuario</title>
</head>
<body>

    <section class="centrar-imagen">
        <img src="ima/logo.PNG" class="logo-menu">
    </section>
    <header class="Tituloosm">   
        <section class="barra">
            <h2>Menu<img src="ima/lista_icono2.png" alt=""></h2>
            <ul>
                <li><a href="/pedido">Pedido</a></li>
                <li><a href="/proveedor">Proveedor</a></li>
                <li><a href="/servicios">Servicios</a></li>
                <li><a href="/usuarios">Usuarios</a></li>
                <li><a href="/categoria">Categoria</a></li>
                <li><a href="/cita">Cita</a></li>
                <li><a href="/detallecita">Detalle cita</a></li>
                <li><a href="/detallepedido">Detalle pedido</a></li>
                <li><a href="/dueno">Due√±o</a></li>
                <li><a href="/insumos">Insumos</a></li>
                <li><a href="/mascota">Mascota</a></li>
                <li><a href="/producto">Producto</a></li>
                <li><a href="/ventas">Ventas</a></li>
                <li><a href="/veterinario">Veterinario</a></li>
            </ul>
           
        </section> 
    </header>
    <nav class="menu">
        <a href="cerrar">Cerrar sesion</a>
    </nav>
    <div class="login-box">
        <h1>Insumos</h1>
        <form action="/frminsertarinsumos" method="POST">
          <!--  INPUT -->
          <input type="text" name="InsuId" class="" placeholder="Id Insumo" >
          <input type="text" name="InsuIdProdu" class="" placeholder="Id Producto">
          <input type="text" name="InsuIdServiUsado" class="" placeholder="Id Servivio Usado">
          <input type="text" name="InsuNombre" class="" placeholder="Nombre">
          <label class="tit">Fecha de Uso del Insumo:</label>
          <input type="date" name="InsuFechaUsado"  id="fecha"  class="" placeholder="Fecha de Uso del Insumo">
          <!--  BUTTON -->
          <button type="submit">Insertar</button>
        </form>   					
      </div>

      <from action="" name="frm" id="frm" class="tabla">
        <section class="scroll">
        <table class="table">
            <thead>
                <tr>
                    <th>ID INSUMO</th>
                    <th>ID PRODUCTO</th>
                    <th>ID SERVICIO</th>
                    <th>NOMBRE</th>
                    <th>FECHA USADO</th>
                    <th>ACTUALIZAR</th>
                </tr>
            </thead>
    
            <tbody>
             <% datos.forEach(function (datos){ %>    
                <tr>
                    <td><input type="text" value="<%=datos.InsuId%>" name="ins" id="ins" class="ins"></td>
                    <td><input type="text" value="<%=datos.InsuIdProdu%>" name="pro" id="pro" class="pro"></td>
                    <td><input type="text" value="<%=datos.InsuIdServiUsado%>" name="ser" id="ser" class="ser"></td>                
                    <td><input type="text" value="<%=datos.InsuNombre%>" name="nom" id="nom" class="nom"></td> 
                    <td><input type="text" value="<%=datos.InsuFechaUsado%>" name="fec" id="fec" class="fec"></td> 
                    <td><input type="submit" value="Actualizar" name="btnact" id="btnact" class="btnact"></td>                              
                </tr>
             <%})%>
            </tbody>
        </table>
    </section>
    </from>

</body>
</html>